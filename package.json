{
  "name": "wxml-transpiler",
  "version": "0.1.4",
  "description": "wxml transpiler",
  "main": "packages/wxml-transpiler/index.js",
  "typings": "types/index.d.ts",
  "files": [
    "src",
    "dist/*.js",
    "types/*.d.ts"
  ],
  "scripts": {
    "dev": "rollup -w -c build/config.js --environment TARGET:web-compiler ",
    "dev:test": "karma start test/unit/karma.dev.config.js",
    "dev:compiler": "rollup -w -c build/config.js --environment TARGET:web-compiler ",
    "build": "node build/build.js",
    "test": "node test/test.js",
    "test:types": "tsc -p ./types/test/tsconfig.json",
    "lint": "eslint src build test",
    "flow": "flow check",
    "sauce": "karma start test/unit/karma.sauce.config.js",
    "bench:ssr": "npm run build:ssr && node benchmarks/ssr/renderToString.js && node benchmarks/ssr/renderToStream.js",
    "release": "bash build/release.sh",
    "release:note": "node build/gen-release-note.js",
    "setup": "node build/install-hooks.js",
    "commit": "git-cz"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/IOriens/wxml-transpiler.git"
  },
  "keywords": [
    "wxml",
    "transpiler"
  ],
  "author": "Junjie Ding",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/IOriens/wxml-transpiler/issues"
  },
  "homepage": "https://github.com/IOriens/wxml-transpiler",
